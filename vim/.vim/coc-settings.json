{
    "coc.preferences.colorSupport": true,
    "coc.preferences.formatOnSaveFiletypes": ["handlebars"],
    "coc.preferences.rootPatterns": [
        "package.json",
        "tsconfig.json",
        "jsconfig.json",
        ".projectroot",
        ".vim",
        ".git"
    ],
    "diagnostic.checkCurrentLine": true,
    "diagnostic.displayByAle": false,
    "diagnostic.errorSign": "✗",
    "diagnostic.infoSign": "ℹ",
    "diagnostic.messageDelay": 200,
    "diagnostic.warningSign": "⚠",
    "eslint.autoFixOnSave": true,
    "eslint.nodePath": ".yarn/sdks",
    "eslint.packageManager": "yarn",
    "explorer.file.reveal.auto": true,
    "explorer.file.child.template": "[selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5][size]",
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 30,
    "explorer.keyMappings.global": {
        "<cr>": [
            "expandable?",
            ["expanded?", "collapse", "expand"],
            "open"
        ]
    },
    "javascript.suggestionActions.enabled": false,
    "languageserver": {
        "bash": {
            "args": [
                "start"
            ],
            "command": "bash-language-server",
            "filetypes": [
                "sh"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        }
    },
    "python.jediEnabled": false,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de keyword",
        "variable": "\ue79b variable",
        "value": "\uf89f value",
        "operator": "\u03a8 operator",
        "constructor": "\uf0ad constructor",
        "function": "\u0192 function",
        "reference": "\ufa46 reference",
        "constant": "\uf8fe constant",
        "method": "\uf09a method",
        "struct": "\ufb44 struct",
        "class": "\uf0e8 class",
        "interface": "\uf417 interface",
        "text": "\ue612 text",
        "enum": "\uf435 enum",
        "enumMember": "\uf02b member",
        "module": "\uf40d module",
        "color": "\ue22b color",
        "property": "\ue624 property",
        "field": "\uf9be field",
        "unit": "\uf475 unit",
        "event": "\ufacd event",
        "file": "\uf723 file",
        "folder": "\uf114 folder",
        "snippet": "\ue60b snippet",
        "typeParameter": "\uf728 type",
        "default": "\uf29c default"
    },
    "suggest.enablePreview": true,
    "suggest.maxCompleteItemCount": 10,
    "suggest.timeout": 5000,
    "tsserver.disableAutomaticTypeAcquisition": false,
    "tsserver.maxTsServerMemory": 3072,
    "typescript.updateImportsOnFileMove.enable": false,
    "homeassistant.hostUrl": "http://192.168.1.142:8123",
